** start of script.js **

let adjective = "Test Adjective Variable";
let noun = "Test Noun Variable";
let verb = "Test Verb Variable";
let place = "Test Place Variable";
let adjective2 = "Test Adjective2 Variable";
let noun2 = "Test Noun2 Variable";

let firstStory = "Once upon a time, there was a(n) " + adjective + " " + noun + " who loved to eat " + noun2 + ". The " + noun + " lived in a " + place + " and had " + adjective2 + " nostrils that blew fire when it was " + verb + ".";
console.log("First story: " + firstStory);

// Reassign variables for second story
adjective = "Test1 Adjective Variable";
noun = "Test1 Noun Variable";
verb = "Test1 Verb Variable";
place = "Test1 Place Variable";
adjective2 = "Test1 Adjective2 Variable";
noun2 = "Test1 Noun2 Variable";

let secondStory = "Once upon a time, there was a(n) " + adjective + " " + noun + " who loved to eat " + noun2 + ". The " + noun + " lived in a " + place + " and had " + adjective2 + " nostrils that blew fire when it was " + verb + ".";
console.log("Second story: " + secondStory);


** end of script.js **

